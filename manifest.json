{
  "manifest_version": 2,
  "name": "LyricFinder",
  "version": "1.0",
  "description": "Displays Lyrics for Youtube Videos",

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["jquery-3.3.1.min.js", "bootstrap.min.js"]
  },

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}
